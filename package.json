{
  "name": "sveltycms",
  "description": "A powerful Headless CMS with sveltekit power",
  "version": "0.0.5",
  "scripts": {
    "-- INSTALLER -----------------------------------------": "",
    "installer": "node cli-installer/cli-installer.js",
    "-- DEVELOP -------------------------------------------": "",
    "dev": "vite dev",
    "devv": "LAUNCH_EDITOR='code' vite --open",
    "-- BUILD ---------------------------------------------": "",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "-- CHECK & TEST ---------------------------------------": "",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
    "check:watch": " svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "paraglide-js compile && npm test",
    "test:bun": "bun test tests/bun",
    "test:playwright": "npx playwright test tests/playwright",
    "test:all": "npm run test && npm run test:playwright",
    "-- LINT & FORMAT ---------------------------------------": "",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "-- OTHER ----------------------------------------------": "",
    "preview": "NODE_OPTIONS='--max-old-space-size=8192' vite preview",
    "paraglide": "paraglide-js compile --project ./project.inlang",
    "translate": "npx @inlang/cli machine translate --project ./project.inlang",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SveltyCMS/SveltyCMS"
  },
  "bugs": {
    "url": "https://github.com/SveltyCMS/SveltyCMS/issues"
  },
  "keywords": [
    "cms",
    "content-management-system",
    "DXP Digital",
    "Developer Experience Platform",
    "inlang",
    "i18n",
    "internationalization",
    "paraglide-js",
    "headless",
    "headless-cms",
    "graphql",
    "skeleton",
    "svelte",
    "svelte-cms",
    "sveltekit",
    "sveltekit_cms",
    "sveltycms",
    "oauth",
    "tailwind",
    "tailwindcss",
    "rich-text-editor",
    "lexical-editor",
    "mongodb"
  ],
